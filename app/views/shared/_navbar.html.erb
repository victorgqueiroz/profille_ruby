<%= content_tag :div, class: "content" do %>

  <%= content_tag :navbar, id: "left" do %>
      <%= content_tag :ul do %>
    <%= content_tag :div, id: "container-bar" do %>
       <%= content_tag :li, class: "nav-item about" do %>
          <%= content_tag :h4, "ABOUT ME" %>
          <%= content_tag :p, "WHO I AM" %>
        <% end %>

        <%= content_tag :li, class: "nav-item carrer" do %>
          <%= content_tag :h4, "MY CARRER" %>
          <%= content_tag :p, "WORKS" %>
        <% end %>
      <% end %>
    <% end %>
  <% end %>
<% end %>

 <%= content_tag :div, id: "content-container" do %>

  <%= content_tag :div, id: "about-page" do %>
        <%= render partial: "about"%>
      <% end %>

    <%= content_tag :div, id: "carrer-page" do %>
        <%= render partial: "carrer"%>
      <% end %>

 <% end %>
